<Page x:Class="YogaApp.Pages.Catalogue"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:YogaApp.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Catalogue">

    <Grid>
        <Button Name="btnChild" Content="Child" Style="{DynamicResource AccentedSquareButtonStyle}" Width="60" Height="30" Margin="120,70,120,200" Click="btnChild_Click"></Button>
        <Button Name="btnSenior" Content="Senior" Style="{DynamicResource AccentedSquareButtonStyle}" Width="60" Height="30" Margin="120,114,120,156" Click="btnSenior_Click"></Button>
        <Button Name="btnMale" Content="Male" Style="{DynamicResource AccentedSquareButtonStyle}" Width="60" Height="30" Margin="120,156,120,114" Click="btnMale_Click"></Button>
        <Button Name="btnFemale" Content="Female" Style="{DynamicResource AccentedSquareButtonStyle}" Width="60" Height="30" Margin="120,198,120,70" Click="btnFemale_Click"></Button>
    </Grid>
</Page>
