<Page x:Class="WpfApplication1.TutorialPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApplication1"
      mc:Ignorable="d" 
      d:DesignHeight="670" d:DesignWidth="1000"
      Title="TutorialPage">
    <GroupBox x:Name="groupBox1" Header="Video Section" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Height="650" Width="990">
        <Grid>
            <StackPanel HorizontalAlignment="Left" Width="660" Height="640">
                <WebBrowser x:Name="webBrowser1" HorizontalAlignment="Left" Height="630" VerticalAlignment="Top" Width="660"/>
            </StackPanel>
            <StackPanel HorizontalAlignment="Right" Height="640" VerticalAlignment="Top" Width="330">
                <Button x:Name="button1" Content="Play Video" HorizontalAlignment="Center" VerticalAlignment="Top" Width="143" Click="button1_Click" Height="50" Margin="0,200,0,0"/>
                <Button x:Name="button" Content="Go To Previous Page" HorizontalAlignment="Center" VerticalAlignment="Top" Width="142" Click="button_Click" Height="50" Margin="0,50,0,0"/>
            </StackPanel>
        </Grid>
    </GroupBox>
</Page>
